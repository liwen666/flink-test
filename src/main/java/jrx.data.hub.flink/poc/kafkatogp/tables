create table kods_am_delinquency_info_testy(
`id` BIGINT,
`tenant_id` STRING,
`account_id` STRING,
`organization_id` STRING,
`account_status` STRING,
`product_id` STRING,
`account_type` STRING,
`customer_id` STRING,
`loan_id` STRING,
`billing_tenor` SMALLINT,
`delq_days` SMALLINT,
`delq_date` DATE,
`last_delq_days` SMALLINT,
`total_amount_due` DECIMAL(18,10),
`delq_process_table_id` STRING,
`penalty_table_id` STRING,
`joint_loan_flag` STRING,
`chargeoff_flag` STRING,
`fund_id` STRING,
`channel` STRING,
`create_time` TIMESTAMP,
`update_time` TIMESTAMP,
`update_by` STRING,
`record_version_number` INT,
`binlog_ts` TIMESTAMP(3)
)
WITH(
'connector'='kafka',
'topic'='kods_am_delinquency_info_testy',
'properties.bootstrap.servers'='172.16.102.23:9092',
'properties.group.id'='ods_group_id',
'format'='debezium-json');
